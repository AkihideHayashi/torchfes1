UNITS ENERGY=eV
p: READ FILE=COLVAR IGNORE_TIME VALUES=p.x
metad: READ FILE=COLVAR IGNORE_TIME VALUES=metad.rbias
weights1: REWEIGHT_METAD TEMP=300
weights2: REWEIGHT_BIAS TEMP=300 ARG=metad.rbias
hh1: HISTOGRAM ARG=p.x GRID_MIN=-0.6 GRID_MAX=0.6 GRID_SPACING=0.1 LOGWEIGHTS=weights1 BANDWIDTH=0.10
hh2: HISTOGRAM ARG=p.x GRID_MIN=-0.6 GRID_MAX=0.6 GRID_SPACING=0.1 LOGWEIGHTS=weights2 BANDWIDTH=0.10

DUMPGRID GRID=hh1 FILE=histo
DUMPGRID GRID=hh2 FILE=histo
